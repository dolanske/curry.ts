    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "csstype": "^3.1.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.9.1",
    "eslint": "^9.14.0",
    "jsdom": "^25.0.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.3.0"
  }
