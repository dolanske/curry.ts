{
  "name": "@dolanske/curry",
  "version": "1.0.0",
  "description": "Fully typed, simple and powerful DOM manipulation library.",
  "author": "dolanske",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dolanske/curry.ts.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "dom-manipulation",
    "jQuery",
    "dom",
    "jquery-clone"
  ],
  "exports": {
    ".": {
      "import": "./dist/curry.mjs"
    }
  },
  "main": "./dist/curry.mjs",
  "module": "./dist/curry.mjs",
  "types": "./dist/src/index.d.ts",
  "files": [
