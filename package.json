    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "csstype": "^3.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.6",
    "@vitest/coverage-c8": "^0.31.0",
    "c8": "^7.13.0",
    "eslint": "^8.40.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.31.0"
  }
